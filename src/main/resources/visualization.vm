<html>
  <head>
    <title>Visualization</title>
    <meta name="decorator" content="atl.userprofile">
    $webResourceManager.requireResource("org.catrobat.confluence.timesheet:visualization-resources")
  </head>
  <body>
    <script type="text/javascript">
      var timesheetID = $timesheetid;
    </script>
    <div id="aui-message-bar"></div>
		
		<div class="aui-flatpack-example">
        <form class="aui">
            <select id="select2-example" multiple>
                <option value="CONF">Confluence</option>
                <option value="JIRA">JIRA</option>
                <option value="BAM">Bamboo</option>
                <option value="JAG">JIRA Agile</option>
                <option value="CAP">JIRA Capture</option>
                <option value="AUI">AUI</option>
            </select>
        </form>

        <script>
            AJS.$("#select2-example").auiSelect2();
        </script>
    </div>

		<div id="visualization-table" class="aui aui-table-interactive table">
			<div class="tr"><span class="td"><span class="aui-icon aui-icon-wait">Loading...</span></span></div>
		</div>
